{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run release"
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build release"
		},
		{
			"type": "cargo",
			"command": "test",
			"args": ["--","--nocapture"],
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "rust: cargo test release"
		}
	]
}